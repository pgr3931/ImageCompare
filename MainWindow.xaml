<Window x:Class="ImageCompareUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageCompareUI.View"
        mc:Ignorable="d"
        Title="ImageCompare" MinWidth="920" MinHeight="500" Height="500" Width="920"
        WindowState="Maximized">
    <Window.Background>
        <ImageBrush ImageSource="Resources/bg.png" Stretch="Fill" Opacity="0.4" />
    </Window.Background>
    <Grid Grid.Column="0" Margin="10">
        <local:ComparisonView/>
    </Grid>
</Window>
